{
  "name": "aimee-ai-system",
  "version": "1.0.0",
  "description": "Aimee Best Friend AI - Complete SMS and Web Platform System",
  "private": true,
  "scripts": {
    "dev": "cd aimee-web && npm run dev",
    "build": "cd aimee-web && npm run build",
    "start": "cd aimee-web && npm run start",
    "lint": "cd aimee-web && npm run lint",
