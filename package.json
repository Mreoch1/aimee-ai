    "@supabase/supabase-js": "^2.50.2",
    "twilio": "^5.3.5",
    "openai": "^4.73.1",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "nodemon": "^3.1.9"
  },
  "engines": {
    "node": ">=18.0.0"
  }
